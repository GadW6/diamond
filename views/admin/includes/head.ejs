<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>YW | <%- title -%></title>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700&display=swap" rel="stylesheet">
    <link
        href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
        rel="stylesheet">

    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
        integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />

    <link rel="shortcut icon" href="/img/icon-diamond.png" type="image/x-icon">

    <!-- Custom Bootstrap 4 -->
    <link href="/css/sb-admin-2.min.css" rel="stylesheet">

    <link rel="stylesheet" href="/css/fix-template.css">

    <!-- Global Custom CSS -->
    <link rel="stylesheet" href="/css/admin/global.css">

    <!-- Custom CSS for routes -->
    <% if(typeof page !== 'undefined' && isAssetExists(page, 'css')) { %> 
        <link rel="stylesheet" href="/css/admin/<%- `${page}/${page}` -%>.css">
    <% } %>
    <% if (typeof slots !== 'undefined' && slots.length > 0 && typeof page !== 'undefined') slots.forEach(slot => { %>
        <%- (isAssetExists(slot, 'css', page)) ? `<link rel="stylesheet" href="/css/admin/${page}/${slot}.css">` : '' -%>
     <% }) %>

</head>