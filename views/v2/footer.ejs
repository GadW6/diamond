<!-- Footer -->
<footer id="footer" class="panel">
  <div class="inner split">
    <div>
      <section>
        <h2><%- site.titleHero -%></h2>
        <p><%- site.subTitleHero -%></p>
      </section>
      <div class="information">
        <section>
          <h4><%- site.authQuote -%></h4>
          <ul class="icons">
            <li><a href="https://www.facebook.com/yair.weintraub.1" class="icon brands fa-facebook-f"><span class="label">Yair Weintraub</span></a></li>
            <li><a href="mailto:<%- site.email -%>" class="icon brands"><i class="fas fa-envelope"></i><span class="label"><%- site.email -%></span></a></li>
            <li><a href="tel:00<%- site.phone -%>" class="icon brands"><i class="fas fa-phone-alt"></i><span class="label">00<%- site.phone -%></span></a></li>
          </ul>
          <h6><strong>Address:</strong></h6><p><%- site.baseAdd -%>, <%- site.cityAdd -%>, <%- site.countryAdd -%></p>
        </section>
        <section>
          <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2499.213063458087!2d4.418070815759237!3d51.215150579588126!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47c3f703c7ad1681%3A0x85471a9dcd77208a!2sPelikaanstraat%2062%2C%202018%20Antwerpen%2C%20Belgium!5e0!3m2!1sen!2sil!4v1605965557696!5m2!1sen!2sil" width="" height="" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
        </section>
      </div>

      <section>
        
      </section>
      <p class="legal"><a href="/terms" style="margin-right: 15px;">Terms & Conditions</a><a href="/privacy">Privacy</a></p>
      <p class="copyright">
        &copy; YW | DIAMOND - <%- new Date().getFullYear() -%>.  Developed By <a href="https://portfolio.itcloudpro.com">ITCLOUDPRO</a> & Designed By: <a href="http://html5up.net">HTML5 UP</a>.
      </p>
    </div>
    <div>
      <section>
        <h2>Get in touch</h2>
        <form method="post" action="#">
          <div class="fields">
            <div class="field half">
              <input type="text" name="name" id="name" placeholder="Name" />
            </div>
            <div class="field half">
              <input type="text" name="email" id="email" placeholder="Email" />
            </div>
            <div class="field">
              <textarea name="message" id="message" rows="4" placeholder="Message"></textarea>
            </div>
          </div>
          <ul class="actions">
            <li><input type="button" onclick="sendMail(event)" value="Send" class="primary" /></li>
            <li><input type="reset" value="Reset" /></li>
          </ul>
          <p style="margin: 0;" id="msgText"></p>
          <div class="loadingio-spinner-rolling-mw05eust31m">
            <div class="ldio-1vvp1rn49sv">
              <div></div>
            </div>
          </div>
        </form>
      </section>
    </div>
  </div>
</footer>
